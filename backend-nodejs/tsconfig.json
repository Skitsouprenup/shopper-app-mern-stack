{
    "compilerOptions": {
      "esModuleInterop": true,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "strict": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "forceConsistentCasingInFileNames": true,
      "target": "ESNext",
      "rootDir": "./src",
      "outDir": "./dist",
    },
    "exclude": ["node_modules"],
    "include": ["src"],
  }